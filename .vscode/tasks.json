{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "graph data",
            "type": "shell",
            "command": "git ls-files \"testData/*.expand\" | xargs cally > out.dot"
        },  
        {
            "label": "show data",
            "type": "shell",
            "command": "cat out.dot",
            "dependsOn":["graph data"]
        },      
        {
            "label": "graph image",
            "type": "shell",
            "command": "cat out.dot | dot -Grankdir=LR -Tpng -o full_caller.png && mimeopen full_caller.png",
            // "dependsOn":["show data"]
        },
    ]
}