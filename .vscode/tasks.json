{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "graph data",
            "type": "shell",
            "command": "find . -type f -name \"*.expand\" | xargs cally --no-externs | > out.txt"
        },        
        {
            "label": "graph image",
            "type": "shell",
            "command": "cat out.txt | dot -Grankdir=LR -Tpng -o full_caller.png",
            "dependsOn":["graph data"]
        },
    ]
}